{
  "name": "BaconTools",
  "main": "./lib/bacontools.js",
  "version": "1.2.1",
  "description": "Ad Hoc. 'Bacon tools' for Atom. Branched of based on autocompletion for LaTeX. (https://atom.io/packages/latex-autocomplete, https://github.com/Evpok/latex-autocomplete)",
  "keywords": [
    "LaTeX",
    "autocompletion"
  ],
  "activationCommands": {
    "atom-workspace": [
      "bacontools:List Unimplemented",
      "bacontools:List Missing"
    ]
  },
  "repository": "https://github.com/Strauman/latex-autocomplete",
  "license": "wtfpl",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "escape-regexp": "^0.0.1",
    "atom-space-pen-views": ">=2.2.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "get_autocomplete_providers"
      }
    }
  }
}
